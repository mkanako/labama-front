{
  "name": "labama-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint \"src/**/*.{vue,less}\" --fix",
    "inspect": "output='webpack.config.js';ramdisk='/Volumes/ramdisk/';if [[ -e $ramdisk ]];then output=\"$ramdisk$output\";else output=\"$(pwd)/$output\";fi;(printf \"module.exports =\"&&vue-cli-service inspect --mode production) > $output && echo $output",
    "inspect:dev": "output='webpack.config.js';ramdisk='/Volumes/ramdisk/';if [[ -e $ramdisk ]];then output=\"$ramdisk$output\";else output=\"$(pwd)/$output\";fi;(printf \"module.exports =\"&&vue-cli-service inspect) > $output && echo $output",
    "ram-cache": "result=$(node -e 'const fs=require(\"fs-extra\");const RAM_DISK=\"/Volumes/ramdisk/\";const DIR=RAM_DISK+process.env.npm_package_name;fs.existsSync(RAM_DISK)&&(fs.ensureDirSync(DIR),console.log(DIR))') && if [ ! -z $result ]; then bindfs $result node_modules/.cache; fi",
    "umount": "umount node_modules/.cache",
    "tailwind": "output='output.css';ramdisk='/Volumes/ramdisk/';if [[ -e $ramdisk ]];then output=\"$ramdisk$output\";else output=\"$(pwd)/$output\";fi;tailwind build ./src/styles/tailwind.css -o $output && echo $output"
  },
  "dependencies": {
    "ant-design-vue": "^1.5.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "nprogress": "^0.2.0",
    "ramda": "^0.27.0",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "inline-svgo-loader": "^3.0.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "stylelint": "^13.3.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
